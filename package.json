{
  "name": "@bszhct/component-react",
  "version": "0.0.1",
  "description": "通用的 React 脚手架",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "build": "rm -rf dist && cross-env NODE_ENV=production webpack --config webpack.config.js",
    "eslint": "eslint . --ext .ts,.tsx",
    "es:fix": "eslint . --ext .ts,.tsx --fix",
    "analyzer": "webpack-bundle-analyzer dist/stats.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run eslint",
      "git add"
    ]
  },
  "main": "src/index.ts",
  "license": "ISC",
  "author": "bszhct",
  "dependencies": {
    "@bszhct/template-base": "0.0.1",
    "@reach/router": "1.2.1",
    "axios": "0.19.0",
    "mobx": "4.14.1",
    "mobx-react": "5.4.4",
    "react": "16.11.0",
    "react-dom": "16.11.0"
  },
  "devDependencies": {
    "@bszhct/template-eslint": "0.0.2",
    "@bszhct/template-webpack": "0.0.5",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "mobx-react-devtools": "4.2.15"
  },
  "browserslist": [
    "last 5 Chrome versions",
    "last 5 Firefox versions",
    "Safari >= 6",
    "ie > 8"
  ],
  "files": [
    "lib",
    "dist"
  ],
  "eslintIgnore": [
    "dist"
  ]
}
